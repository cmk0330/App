<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">



    <TextView
        android:id="@+id/tv_launch"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_gravity="center"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="32dp"
        android:background="@color/red"
        android:text="launch"
        android:textColor="@color/black"
        android:textSize="32sp" />

    <WebView
        android:id="@+id/webView"
        android:layout_width="match_parent"
        android:visibility="gone"
        android:layout_height="match_parent"/>

    <!--    <LinearLayout-->
    <!--        android:layout_width="match_parent"-->
    <!--        android:layout_height="wrap_content"-->
    <!--        android:orientation="horizontal">-->
    <!--        <com.cmk.app.custom.CustomCircle-->
    <!--            android:layout_width="wrap_content"-->
    <!--            android:layout_height="300dp"/>-->
    <!--    </LinearLayout>-->

    <com.cmk.app.widget.LoadingProgress
        android:id="@+id/loadingProgress"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_gravity="center_horizontal"
        android:padding="10dp"
        app:annulusBackgroundColor="@color/green"
        app:annulusColor="@color/main"
        app:annulusWidth="20dp"
        app:innerColor="@color/blue"
        app:loadingProgressSize="10dp"
        app:percentColor="@color/black"
        app:percentSize="24sp" />


</LinearLayout>